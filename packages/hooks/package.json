{
  "name": "@flowind/hooks",
  "version": "0.2.1",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "files": [
    "dist/**"
  ],
  "license": "MIT",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "sideEffects": false,
  "homepage": "https://flowind.dev",
  "repository": {
    "url": "https://github.com/flowinddev/flowind.git",
    "type": "git",
    "directory": "packages/hooks"
  },
  "keywords": [
    "react",
    "next",
    "nextjs",
    "library",
    "frontend",
    "react-hooks",
    "hooks",
    "state"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:js:cjs && pnpm build:js:esm",
    "build:js:cjs": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:js:esm": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "clean": "rimraf dist",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "lint": "eslint \"**/*.ts*\""
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {},
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
